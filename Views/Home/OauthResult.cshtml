
@{
    ViewBag.Title = "OauthResult";
}

<h2>OauthResult</h2>
<table>
    <thead>
        <tr>
            <td>Key</td>
            <td>Value</td>
        </tr>
        <tr>
            <td>Facebook API ID</td>
            <td>@ViewBag.ProviderUserID</td>
        </tr>
        <tr>
            <td>Gender</td>
            <td>@ViewBag.Gender</td>
        </tr>
        <tr>
            <td>Name</td>
            <td>@ViewBag.Name</td>
        </tr>
        <tr>
            <td>Access token</td>
            <td>@ViewBag.AccessToken</td>
        </tr>
    </thead>
</table>
